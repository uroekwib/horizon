{% doc %}
  @prompt
    Create a dynamic table section with 7-8 columns and rows using class desc_container page-width for table width constraint (not full viewport width). Each cell should support both images and text on separate lines. Features needed: add/remove rows and columns, sticky first column when scrolling horizontally, and comprehensive theme customizer settings. The table should be highly flexible and user-friendly with proper width constraints within the desc_container page-width class.
    
    
{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-dynamic-table-{{ ai_gen_id }} {
    display: block;
    width: 100%;
  }

  .ai-dynamic-table-container-{{ ai_gen_id }} {
    width: 100%;
    overflow-x: auto;
    position: relative;
    border-radius: {{ block.settings.table_border_radius }}px;
    background-color: {{ block.settings.table_background }};
  }

  .ai-dynamic-table-wrapper-{{ ai_gen_id }} {
    min-width: 100%;
    display: inline-block;
  }

  .ai-dynamic-table-element-{{ ai_gen_id }} {
    width: 100%;
    border-collapse: collapse;
    border-spacing: 0;
    font-size: {{ block.settings.text_size }}px;
    color: {{ block.settings.text_color }};
  }

  .ai-dynamic-table-element-{{ ai_gen_id }} th,
  .ai-dynamic-table-element-{{ ai_gen_id }} td {
    padding: {{ block.settings.cell_padding }}px;
    text-align: {{ block.settings.text_alignment }};
    vertical-align: {{ block.settings.vertical_alignment }};
    border: {{ block.settings.border_width }}px solid {{ block.settings.border_color }};
    min-width: {{ block.settings.min_column_width }}px;
  }

  .ai-dynamic-table-element-{{ ai_gen_id }} th {
    background-color: {{ block.settings.header_background }};
    color: {{ block.settings.header_text_color }};
    font-weight: {{ block.settings.header_font_weight }};
    font-size: {{ block.settings.header_text_size }}px;
  }

  .ai-dynamic-table-element-{{ ai_gen_id }} tbody tr:nth-child(even) {
    background-color: {{ block.settings.row_alternate_background }};
  }

  .ai-dynamic-table-element-{{ ai_gen_id }} tbody tr:hover {
    background-color: {{ block.settings.row_hover_background }};
  }

  {% if block.settings.sticky_first_column %}
    .ai-dynamic-table-element-{{ ai_gen_id }} th:first-child,
    .ai-dynamic-table-element-{{ ai_gen_id }} td:first-child {
      position: sticky;
      left: 0;
      z-index: 2;
      background-color: {{ block.settings.header_background }};
    }

    .ai-dynamic-table-element-{{ ai_gen_id }} tbody td:first-child {
      background-color: {{ block.settings.table_background }};
    }

    .ai-dynamic-table-element-{{ ai_gen_id }} tbody tr:nth-child(even) td:first-child {
      background-color: {{ block.settings.row_alternate_background }};
    }

    .ai-dynamic-table-element-{{ ai_gen_id }} tbody tr:hover td:first-child {
      background-color: {{ block.settings.row_hover_background }};
    }
  {% endif %}

  .ai-dynamic-table-cell-content-{{ ai_gen_id }} {
    display: flex;
    flex-direction: column;
    gap: 8px;
    align-items: {{ block.settings.text_alignment }};
  }

  .ai-dynamic-table-cell-image-{{ ai_gen_id }} {
    width: {{ block.settings.image_width }}px;
    height: {{ block.settings.image_height }}px;
    object-fit: cover;
    border-radius: {{ block.settings.image_border_radius }}px;
  }

  .ai-dynamic-table-cell-text-{{ ai_gen_id }} {
    line-height: 1.5;
  }

  @media screen and (max-width: 749px) {
    .ai-dynamic-table-element-{{ ai_gen_id }} th,
    .ai-dynamic-table-element-{{ ai_gen_id }} td {
      padding: {{ block.settings.cell_padding | times: 0.7 }}px;
      font-size: {{ block.settings.text_size | times: 0.9 }}px;
    }

    .ai-dynamic-table-element-{{ ai_gen_id }} th {
      font-size: {{ block.settings.header_text_size | times: 0.9 }}px;
    }

    .ai-dynamic-table-cell-image-{{ ai_gen_id }} {
      width: {{ block.settings.image_width | times: 0.8 }}px;
      height: {{ block.settings.image_height | times: 0.8 }}px;
    }
  }
{% endstyle %}

<dynamic-table-{{ ai_gen_id }} class="ai-dynamic-table-{{ ai_gen_id }} desc_container page-width" {{ block.shopify_attributes }}>
  <div class="ai-dynamic-table-container-{{ ai_gen_id }}">
    <div class="ai-dynamic-table-wrapper-{{ ai_gen_id }}">
      <table class="ai-dynamic-table-element-{{ ai_gen_id }}">
        <thead>
          <tr>
            {% for i in (1..8) %}
              {% liquid
                assign header_key = 'header_' | append: i
                assign header_text = block.settings[header_key]
              %}
              {% if header_text != blank %}
                <th>{{ header_text }}</th>
              {% endif %}
            {% endfor %}
          </tr>
        </thead>
        <tbody>
          {% for row in (1..7) %}
            <tr>
              {% for col in (1..8) %}
                {% liquid
                  assign header_key = 'header_' | append: col
                  assign header_text = block.settings[header_key]
                  
                  assign cell_image_key = 'row_' | append: row | append: '_col_' | append: col | append: '_image'
                  assign cell_text_key = 'row_' | append: row | append: '_col_' | append: col | append: '_text'
                  
                  assign cell_image = block.settings[cell_image_key]
                  assign cell_text = block.settings[cell_text_key]
                %}
                {% if header_text != blank %}
                  <td>
                    <div class="ai-dynamic-table-cell-content-{{ ai_gen_id }}">
                      {% if cell_image != blank %}
                        <img
                          src="{{ cell_image | image_url: width: 400 }}"
                          alt="{{ cell_text | escape }}"
                          class="ai-dynamic-table-cell-image-{{ ai_gen_id }}"
                          loading="lazy"
                          width="{{ block.settings.image_width }}"
                          height="{{ block.settings.image_height }}"
                        >
                      {% endif %}
                      {% if cell_text != blank %}
                        <div class="ai-dynamic-table-cell-text-{{ ai_gen_id }}">{{ cell_text }}</div>
                      {% endif %}
                    </div>
                  </td>
                {% endif %}
              {% endfor %}
            </tr>
          {% endfor %}
        </tbody>
      </table>
    </div>
  </div>
</dynamic-table-{{ ai_gen_id }}>

<script>
  (function() {
    class DynamicTable{{ ai_gen_id }} extends HTMLElement {
      constructor() {
        super();
      }

      connectedCallback() {
        this.container = this.querySelector('.ai-dynamic-table-container-{{ ai_gen_id }}');
        this.setupScrollIndicator();
      }

      setupScrollIndicator() {
        if (!this.container) return;

        this.container.addEventListener('scroll', () => {
          const scrollLeft = this.container.scrollLeft;
          const scrollWidth = this.container.scrollWidth;
          const clientWidth = this.container.clientWidth;

          if (scrollLeft > 0) {
            this.container.style.boxShadow = 'inset 10px 0 10px -10px rgba(0,0,0,0.1)';
          } else {
            this.container.style.boxShadow = 'none';
          }

          if (scrollLeft + clientWidth < scrollWidth - 1) {
            const currentShadow = this.container.style.boxShadow;
            if (currentShadow && currentShadow !== 'none') {
              this.container.style.boxShadow = currentShadow + ', inset -10px 0 10px -10px rgba(0,0,0,0.1)';
            } else {
              this.container.style.boxShadow = 'inset -10px 0 10px -10px rgba(0,0,0,0.1)';
            }
          }
        });
      }
    }

    customElements.define('dynamic-table-{{ ai_gen_id }}', DynamicTable{{ ai_gen_id }});
  })();
</script>

